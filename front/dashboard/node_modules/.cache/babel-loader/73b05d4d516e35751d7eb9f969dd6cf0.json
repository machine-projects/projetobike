\n      showCountdownAlert( ){\n        if (this.alert == false) {\n          this.alert = !this.alert\n          setInterval(()=>{\n            if(this.countDown == 0) {\n              this.countDown = 100\n              this.alert = !this.alert\n              clearInterval()\n            }\n            else {\n              this.countDown--\n            }\n          }, 100)\n        }\n        else {\n          console.log('a popup ainda esta ativa')\n        }\n      }\n    }\n  }\n</script>"]},"metadata":{},"sourceType":"module"}